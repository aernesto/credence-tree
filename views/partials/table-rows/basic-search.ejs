
<tr>
  <td colspan='1' class='width1'></td>
  <td colspan='3' class='width3'>
    <form action='/basic-search' method='get'>
      <input id='basic-search' name='query' rows='1'
      <% if (typeof query !== 'undefined') { %>
        value = '<%= query %>'
        placeholder = '<%= query %>'
      <% } else { %>
        placeholder = 'Search Credence Tree for assertions and arguments...'
      <% } %> >
      </input>
    </form>
  </td>
  <td colspan='1' class='width1'></td>
</tr>
